{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off",
    "no-param-reassign": "off",
    "no-prototype-builtins": "off",
    "no-undef": "off",
    "no-await-in-loop": "off",
    "no-plusplus": "off",
    "no-unused-vars": "off"
  }
}
